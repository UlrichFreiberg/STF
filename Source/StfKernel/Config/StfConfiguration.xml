<?xml version="1.0"?>
<configuration xmlns="http://www.testautomation.dk/stfConfiguration" version="2013.04.21.0">
  <section name="StfKernel">
    <key name="PluginPath" value="%STF_ROOT%\Plugins\" />
    <section name="StfArchiver">
      <key name="ArchiveTestResults" value="false" />
      <key name="ArchiveDestination" value="" />
      <key name="ZipFilename" value="TestLogs.zip" />
      <key name="TempDirectory" value="%STF_ROOT%\Temp\" />
      <key name="ArchiveTopDir" value="%STF_ROOT%\archiveDir" />
      <key name="UseDateTimeInPath" value="true" />
      <key name="UseTestnameInPath" value="true" />
    </section>
    <section name="StfLogger">
      <key name="OverwriteLogFile" value="true" />
      <key name="LogToFile" value="true" />
      <key name="LogTitle" value="LogTitle" />
      <key name="LogFileName" value="c:\temp\Ovid_defaultlog.html" />
      <key name="AlertLongInterval" value="true" />
      <key name="LogLevel" value="debug" />
      <key name="PathToLogoImageFile" value="" />
      <key name="ScreenshotOnLogFail" value="true" />
      <key name="MapNewlinesToBr" value="false" />
      <key name="KeepAliveInterval" value="5" />
    </section>
  </section>
  <section name="Environments" defaultsection="ENV1">
    <section name="ENV1"></section>
    <section name="ENV2"></section>
  </section>
</configuration>